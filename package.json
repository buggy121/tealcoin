{
  "name": "tealcoin",
  "version": "1.0.0",
  "description": "Simple Blockchain implementation in Typescript.",
  "keywords": [
    "tealcoin",
    "blockchain",
    "transactions"
  ],
  "main": "dist/blockchain.js",
  "scripts": {
    "start": "node dist/server/main.js",
    "build:server": "tsc -p tsconfig.server.json",
    "build:frontend": "rollup --config",
    "build": "rm -rf ./dist && npm run build:server && npm run build:frontend",
    "test": "nyc mocha tests/*.js",
    "lint": "eslint src/** tests/**",
    "coverage": "npm run test && nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Teal Team",
  "license": "MIT",
  "dependencies": {
    "debug": "4.1.1",
    "elliptic": "6.5.2"
  },
  "devDependencies": {
    "@types/node": "13.7.4",
    "@types/elliptic": "6.4.12",
    "@types/debug": "4.1.5",
    "@types/koa": "2.11.1",
    "@types/koa-static": "4.0.1",
    "@types/koa__router": "8.0.2",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "mocha": "7.0.1",
    "nyc": "15.0.0",
    "typescript": "3.8.2",
    "koa": "2.11.0",
    "koa-static": "5.0.0",
    "@koa/router": "8.0.8",
    "rollup": "1.31.1",
    "rollup-plugin-vue": "5.1.6",
    "rollup-plugin-typescript2": "0.26.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-bundle-html": "0.2.2",
    "vue": "2.6.11"
  }
}
